<div class="row">
  <div class="col-lg-12">
    <h4>優勝／受賞歴</h4>
    <% unless @prize_histories.blank? %>
      <ul>
        <% @prize_histories.each do |history| %>
        <%    region = history.region.code == 0 ? history.region.name : history.region.name + "地区" %>
        <li>第 <%= history.contest.nth %> 回 <%= history.contest.name %> <%= region %>大会 <%= history.prize.name %></li>
        <% end %>
      </ul>
    <% end %>
  </div>
</div>
